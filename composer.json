  "type": "yii2-extension",
  "keywords": [
    "CKEditor",
    "wysiwyg",
    "widget"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "MrAnger",
      "email": "slava.sharapov1992@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/MrAnger/yii2-ckeditor/issues",
